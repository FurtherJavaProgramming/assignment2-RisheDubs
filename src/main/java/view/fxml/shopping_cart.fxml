<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="CENTER" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.ShoppingCartController">
    
    <TableView fx:id="cartTable">
        <columns>
            <TableColumn text="Book Title" fx:id="titleColumn"/>
            <TableColumn text="Quantity" fx:id="quantityColumn"/>
        </columns>
    </TableView>

    <Button text="Back" onAction="#returnToDashboard"/>
</VBox>
