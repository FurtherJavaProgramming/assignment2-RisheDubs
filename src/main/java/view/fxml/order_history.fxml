<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="controller.OrderHistoryController" spacing="10" alignment="CENTER">
    
    <TableView fx:id="orderTable">
        <columns>
            <TableColumn text="Order Number" fx:id="orderNumberColumn"/>
            <TableColumn text="Total Price" fx:id="totalPriceColumn"/>
        </columns>
    </TableView>

    <Button text="Back" onAction="#returnToDashboard"/>
</VBox>
